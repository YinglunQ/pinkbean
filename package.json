{
  "name": "pinkbean",
  "version": "2.0.0",
  "description": "A discord bot for MapleStory",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "formatAll": "prettier --write \"src/**/*.ts\"",
    "format": "prettier --write",
    "lint": "eslint . --ext .js,.ts",
    "watch-nodemon": "nodemon --watch dist --inspect=0.0.0.0:9229 --nolazy",
    "watch-tsc": "tsc && tsc -w",
    "test": "mocha --reporter dot --require ts-node/register test/**/*.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js"
  },
